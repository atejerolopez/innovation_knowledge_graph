    <!-- http://inneo.linkeddata.es#dynamism -->

    <owl:DatatypeProperty rdf:about="http://inneo.linkeddata.es#dynamism">
        <rdfs:domain rdf:resource="http://inneo.linkeddata.es#InnovationEcosystem"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment xml:lang="en">Dynamism is defined as the capacity of movement of knowledge through actors and their relationships within the innovation ecosystem</rdfs:comment>
        <rdfs:label xml:lang="en">dynamism</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://inneo.linkeddata.es#efficiency -->

    <owl:DatatypeProperty rdf:about="http://inneo.linkeddata.es#efficiency">
